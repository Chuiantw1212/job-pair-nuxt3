{
  "functions": {
    "source": ".output/server"
  },
  "hosting": {
    "site": "job-pair-taiwan-dev",
    "public": ".output/public",
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "function": "server"
      }
    ],
    "headers": [
      {
        "source": "**/*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=7200"
          },
          {
            "key": "X-Frame-Options",
            "value": "SAMEORIGIN"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "Strict-Transport-Security",
            "value": "max-age=63072000; includeSubDomains"
          },
          {
            "key": "Permissions-Policy",
            "value": "geolocation=*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "*"
          }
        ]
      },
      {
        "source": "**/*.@(js|webp|svg)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      }
    ]
  }
}